[Unit]
Description=Scope service
After=network-online.target

[Install]
WantedBy=multi-user.target

[Service]
Type=simple
ExecStart=/home/pi/piscope/joystick.py
User=pi
Restart=always
Environment=PYTHONPATH=/home/pi/gamepad_qobject/gamepad_qobject:/home/pi/mqtt_qobject/mqtt_qobject:/home/pi/qt_grbl

